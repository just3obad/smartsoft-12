<%= form_for @admin_session do |f| %>
  <% @admin_session.errors.full_messages.each do |msg| %>
      <p><%= msg %></p>
   <% end %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p><%= f.submit "Log in" %></p>
<% end %> 
